/*左右行样式*/
.modelLia{width: 100%;background-color: #fff;height:1.33rem;line-height: 1.33rem;border-bottom: 1px #e8ecf0 solid;box-sizing:border-box;padding: 0 0.4rem;clear: both;display: table;font-size: 16px;}
.modelLia .title-a{font-weight: normal;display: table-cell;width: 2.8rem;text-align: left;font-size:0.4rem;}
.modelLia .title-a.sm{width: 0.9rem;}
.modelLia .title-a.bg{width: 2.6rem;}
.modelLia .title-a.w225{width: 3rem;}
.modelLia .title-a.wid-reset{ width: 3.47rem}
.modelLia .con-a{overflow: hidden;display: table-cell;vertical-align: middle;font-size: 0.33rem}
.modelLia .con-b span{font-size: 0.4rem}
.modelLia .con-b{overflow: hidden;display: table-cell;text-align: right;vertical-align: middle;font-size: 0.4rem}
.modelLia input{width: 96%;height: 1rem;border:none}
.modelLia .pw-jt-icon{
	background-image: url('../../images/icon/pw_04.png');
	padding:0.12rem;
}
a.modelLia{color: #333;text-decoration: none!important;}
a.modelLia:active{background-color: #f6f6f6;}
.tips-font{font-size:0.29rem; color:#5f616b;}

/*图片验证码ff9f86*/
