.modelLia.imgCode .con-b img{width:2.24rem;height: 0.92rem;}


/*段落文字a*/
.modelTexta{width: 100%;height:1.33rem;line-height: 1.33rem;border-bottom: 1px #e8ecf0 solid;box-sizing:border-box;padding: 0 0.4rem;clear: both;display: table;}
.bg-white{background: #fff;}
.modelTexta .con-lt,.modelTexta .con-rt,.modelTexta .con-w100{display: inline-block;}
.modelTexta .con-lt{float: left;font-size: 14px;}
.modelTexta .con-rt{float: right;font-size: 14px;}
.modelTexta .con-w100{width: 100%;font-size: 14px;}
[data-dpr="2"] .modelTexta .con-w100,[data-dpr="2"] .modelTexta .con-lt,[data-dpr="2"] .modelTexta .con-rt{font-size: 28px}
[data-dpr="3"] .modelTexta .con-w100,[data-dpr="3"] .modelTexta .con-lt,[data-dpr="3"] .modelTexta .con-rt{font-size: 42px}