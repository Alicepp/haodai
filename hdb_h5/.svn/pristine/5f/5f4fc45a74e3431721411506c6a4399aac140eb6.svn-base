.bankInfo .infoCont {
  flex: 1;
  -webkit-flex: 1;
  font-size:0.4rem;
  line-height:0.48rem;
}
.bankInfo .bankName {
  margin-bottom:0.187rem;
}
.bankInfo .bankName .grayTxt {
  margin-left:0.267rem;
}
.bankInfo .infoCont .grayTxt {
  color:#afb0be;
  font-size:0.373rem;
}

.fl {float:left;}
.fr {float:right;}
.none {display: none;}
.clearfix:after {
	display:block;
	content: '';
	clear:both;
	overflow:hidden;
}

.isAgree {
	padding:0.333rem 0.4rem 0;
	font-size:0.32rem;
	line-height:0.4rem;
}
.isAgree .protocol {
	margin-right:0.2rem;
	border-radius: 0;
	-webkit-border-radius:0;
}
.isAgree a {
	color:#00a8ff;
}

/*成功提示对话框*/
.pw-manage-wask{
	width: 100%;
	height: 100%;
	background:rgba(0,0,0,.6);
	position: fixed;
	z-index: 99;
	display: none;
}
.pw-manage-wap{
	width: 6.93rem;
	height: 4.27rem;
	position: absolute;
	top:4.67rem;
	left:50%;
	margin-left:-3.47rem;
	background: #fff;
	border-radius: 0.13rem;
	z-index: 111;
	text-align: center;
}

.pw-manage-wap .pw-manage-title{
	font-size:0.48rem;
	color:#5f616b;
	margin-top:0.8rem;
}
.pw-manage-wap .pw-manage-tip{
	font-size:0.4rem;
	color:#afb0be;
	margin:0.53rem 0;
}
.pw-manage-wap .pw-manage-closed{
	border-top:solid 1px #ddd;
	padding-top: 0.27rem;
	display:block;
	font-size: 0.48rem;
	color:#00a8ff;
}

/*提示信息在输入框上面显示开始*/
.Validform_title{display: none;}
.Validform_info{text-align: center;color: #f00;padding-top: 0.4rem;
	animation: bounceInRight 0.8s linear;
	-webkit-animation: bounceInRight 0.8s linear;
	/*animation-iteration-count:1;*/
	/*-webkit-animation-iteration-count:1;*/
}
.customForm{
	position: absolute;
	text-align: center;
	width: 80%;
	left: 10%;
	margin-top: -0.8rem;
	font-size: 0.32rem;
	color:#f00;
	-webkit-animation-duration: .8s!important;
	animation-duration: .8s!important;
}
form{
  position: relative;
}
/*提示信息在输入框上面显示结束*/
/* 去掉上下小箭头按钮 */
input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
    -webkit-appearance: none;    
}
input[type="number"] {
  -moz-appearance: textfield;
}
/*** 弹出页面 ***/
.popPage {
  position: absolute;
  left:0;
  top:0;
  width:100%;
  min-height:100%;
  z-index:100;
  background-color:#f9f9f9;
  /*
  -webkit-overflow-scrolling: touch;
  overflow-scrolling: touch;*/
}
.popPage .header {
  z-index: 100;
  background-color:#fff;
}
.popPage .popPageMain {
  padding-top:1.2rem;
}
/*提示去设置交易密码弹窗样式重置*/
.warmPrompt p {
  padding-bottom:0.733rem;
  font-size:0.4rem;
}
/*标倒计时 在首页、项目页、定期详情页*/
.pro-time {
	border-top:0.013rem solid #eee;
	height:0.747rem;
	line-height:0.733rem;
	font-size:0.32rem;
	color:#afb0be;
	text-align: center;
	overflow:hidden;
	transition: height 0.3s;
	-webkit-transition: height 0.3s;
}
.pro-time .leftTime {
	margin-left:0.107rem;
	color:#8b8e9e;
	font-size:0.293rem;
	vertical-align: top;
}
.pro-time .leftTime .cRed {
	display: inline-block;
	min-width:0.507rem;
	vertical-align: top;
	font-size:0.373rem;
}
/*成功弹层*/
body .sweet-alert.pop-success h2 {
	margin:0;
	height:0;
	border:none;
}
body .sweet-alert.pop-success p {
	padding:0;
}
body .sweet-alert.pop-success .sa-button-container {
	display: block;
}
.pop-success .success-cont {
	padding:0.667rem 0 0.773rem;
	font-size:0.48rem;
	line-height: 1;
}
.pop-success .success-cont .cRed {
	font-size:0.613rem;
}
.pop-success .success-cont .sub-cont {
	margin-top:0.133rem;
	font-size: 0.373rem;
}
.pop-success .success-cont .sub-cont .cRed {
	font-size:0.427rem;
}

.pop-success .success-img {
	margin:0 auto 0.667rem;
	height:3.413rem;
	background-repeat: no-repeat;
	background-position: center;
	background-size: 100% 100%;
}
.pop-success .recharge-img {/*充值*/
	width: 3.733rem;
	background-image: url("../../images/status/recharge_success.png");
}
.pop-success .withdraw-img {/*提现*/
	width:3.307rem;
	background-image: url("../../images/status/cash_success.png");
}
.pop-success .invest-img { /*投标*/
	width:3.36rem;
	background-image: url("../../images/status/invest_success.png");
}
.pop-success .realname-img {/*实名*/
	width:3.64rem;
	background-image: url("../../images/status/real_name_success.png");
}
.pop-success .register-img {/*注册*/
	width:3.467rem;
	background-image: url("../../images/status/register_success.png");
}
.pop-success .rollout-img {/*转出*/
	width:3.36rem;
	background-image: url("../../images/status/roll_out_success.png");
}
.pop-success .shift-img {/*转入*/
	width:3.36rem;
	background-image: url("../../images/status/shift_to_success.png");
}
.pop-success .bindcard-img {/*绑卡*/
	width:3.307rem;
	background-image: url("../../images/status/tied_card_success.png");
}

/*********** footer ***********/
.footer_ad {
	position:fixed;
	left:0;
	right:0;
	bottom:0;
	padding-left:2.547rem;
	height:1.307rem;
	line-height:1.307rem;
	font-size:0.32rem;
	color:#fff;
	background-color:rgba(0,0,0,0.8);
}
.footer_ad .ad_close {
	display:block;
	position: absolute;
	top:0;
	right:0;
	width:0.907rem;
	height:1.307rem;
	border-left:0.013rem solid #fff;
	background:url("data:image/png;base64,") no-repeat center;
	background-size: 0.4rem 0.4rem; 
}
.footer_ad .hdb_icon {
	display: block;
	position: absolute;
	left:0.547rem;
	top:-0.427rem;
	width:1.613rem;
	height:1.613rem;
	background:url("../../images/common/hdb_logo.png") no-repeat 0 0;
	background-size: 100% 100%; 
}
.footer_ad .down_app {
	 display:inline-block;
	 margin-left:0.507rem;
	 width:2.0rem;
	 height:0.533rem;
	 text-align:center;
	 line-height: 0.533rem;	 
	 font-size:0.347rem;
	 color:#ff8e7b;
	 border:0.013rem solid #ff6159;
	 vertical-align:middle;
	 border-radius:0.253rem/0.267rem;
	 -webkit-border-radius:0.253rem/0.267rem;
}

/*解决input黄色底*/
input:-webkit-autofill {
	-webkit-box-shadow: 0 0 0px 1000px #fff inset;
	border: none!important;
}
input.jsPassWord:-webkit-autofill {
	-webkit-box-shadow: 0 0 0px 1000px #FF7B52 inset;
	border: none!important;
}
input.jsUserPhone:-webkit-autofill {
	-webkit-box-shadow: 0 0 0px 1000px #FF7651 inset;
	border: none!important;
}

