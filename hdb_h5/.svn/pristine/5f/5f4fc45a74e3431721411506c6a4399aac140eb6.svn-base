/*########################rest.css########################*/
@import url('./reset.css?__inline');
/*########################animate.css########################*/
@import url('./animate.css?__inline');
/*########################pageloading.css########################*/
@import url('./pageloading.css?__inline');
/*########################header.css########################*/
@import url('./header.css?__inline');
/*########################sweetalert.css########################*/
@import url('./sweetalert.css?__inline');
* {
	box-sizing: border-box;	/* 增加该属性，可以增加弹性，是滑动更加顺畅 */
	-moz-box-sizing:border-box; /* Firefox */
    -webkit-box-sizing:border-box; /* Safari */
	-webkit-overflow-scrolling: touch;
}
[data-dpr="1"] * {
	border-width: 1px !important
}
[v-cloak] {
	display: none;
}

html, body {
	height: 100%;
}
body{
	cursor: pointer;
}
.appwrap {
	font-size: 0.2933rem;
	position: relative;
	width: 100%;	/*top: 1.2rem;*/
	padding-top: 0.2667rem;	/*bottom: 0;*/
	overflow-y: auto;
	overflow-x: hidden;
	-webkit-box-flex: 1;
	-ms-flex: 1;
	flex: 1;
}

/*隐藏滚动条*/
::-webkit-scrollbar {
	width: 0;
	height: 0;
	display: none;
}
.box {
	display: -webkit-box;
	display: -ms-flexbox;
	display: flex;
}
.flex1 {
	-webkit-box-flex: 1;
	-ms-flex: 1;
	flex: 1;
}

/*文字两端对齐*/
.justify {
	text-align: justify;
	/*word-break: break-all;*/
	stext-justify: distribute-all-lines;                /*ie6-8*/
	text-justify: inter-ideograph;
	-webkit-text-align-last: justify;
}

.container{margin:0.267rem 0;}
.content{padding:0 1rem;}
.center{text-align: center;}
.border-top-1{border-top:1px solid #e8ecf0;}

/*公用margin*/
.mt20{margin-top: 0.267rem;}
.mt30{margin-top: 0.4rem;}
.mt40{margin-top: 0.533rem;}
.mt90{margin-top: 1.2rem;}
.mt100{margin-top: 1.333rem;}
.mt230{margin-top: 3.067rem;}
.ml30{margin-left: 0.4rem;}

/*公用padding*/
.pt20{padding-top: 0.267rem;}
.pt30{padding-top: 0.4rem;}
.pl5{padding-left:0.067rem;}

/*居中*/
.tc{text-align: center;}

/*字号*/
.f28{font-size: 0.373rem;}
.f30{font-size: 0.4rem;}
.f60{font-size: 0.8rem;}

/*红色*/
.cRed{color: #fd716a;}
/*蓝色*/
.cBlue{color: #00a8ff;}
.cBlue:hover{color: #00a8ff;}
/*浅灰色*/
.cGray{color: #afb0be;}
/*深灰色*/
.cGray2{color: #5f616b;}
/*绿色*/
.cGreen{color:#1dd594;}
/*加下划线蓝色*/
.blue_link{color: #00a8ff;text-decoration: underline;}

/*公用icon*/
.icon{background-size: 100%;background-repeat: no-repeat;padding: 0.24rem;background-position: center;}
.icon_mobile{background-image: url(../../images/icon/icon_mobile.png?__inline);} /*手机号*/
.icon_mobile_gray{background-image: url(../../images/icon/icon_mobile_gray.png?__inline);} /*手机号*/
.icon_pwd{background-image: url(../../images/icon/icon_pwd.png?__inline);}  /*密码*/
.icon_pwd_gray{background-image: url(../../images/icon/icon_pwd_gray.png?__inline);}  /*密码*/
.icon_code{background-image: url(../../images/icon/icon_imgcode.png?__inline);}  /*图片验证码*/
.icon_code_gray{background-image: url(../../images/icon/icon_imgcode_gray.png?__inline);}  /*图片验证码*/
.icon_pwd_hui_hide{background-image: url(../../images/icon/pw_hide.png?__inline);padding:0.34rem;} /*密码眼睛隐藏状态*/
.icon_pwd_hui_show{background-image: url(../../images/icon/pw_show.png?__inline);padding:0.34rem;} /*显示状态*/

.icon_user{background-image: url(../../images/icon/icon_user.png?__inline);}  /*用户姓名*/
.icon_idcard{background-image: url(../../images/icon/icon_idcard.png?__inline);padding:0.26rem;}  /*身份证号*/

.icon_moredetail {padding:0.267rem;background-image: url(../../images/icon/icon_moredetail.png?__inline);} /*更多详情*/
.icon_record {padding:0.267rem;background-image: url(../../images/icon/icon_record.png?__inline);} /*记录*/
.icon_income {padding:0.267rem;background-image: url(../../images/icon/icon_income.png?__inline);} /*收益*/
.icon_bulb {padding:0.16rem;background-image: url(../../images/icon/icon_bulb.png?__inline);}
/*新消息红色圆点*/
.newMsg{width: 0.333rem;height: 0.333rem;background-color: #e8544a;border-radius:50%;text-align: center;line-height: 0.333rem;font-size: 0.24rem;position: absolute;}

/*右箭头*/
.rArrow {
	display: inline-block;
	width:0.4rem;
	height:0.4rem;
	border-style: solid;
	border-width:0.04rem 0.04rem 0 0;
	border-color:#dcdcdc #dcdcdc transparent transparent;
	vertical-align: middle;
	-webkit-transform: rotate(45deg);
	        transform: rotate(45deg);
}
/*灯泡提醒*/
.bulbRemind {
	margin-top:0.2rem;
	text-align: center;
	font-size:0.293rem;
	line-height: 1.6;
	color:#c2c1c1;
}
.bulbRemind .icon_bulb {
	margin-right:0.24rem;
}
/*填写表单样式*/
.infoItem {
	display: block;
	padding:0 0.373rem;
	height:1.32rem;
	line-height:1.28rem;
	font-size:0.373rem;
	border-bottom:0.013rem solid #eee;
	background-color:#fff;
}
.infoItem .infoName {
	float:left;
	margin-right:0.373rem;
}
.infoItem .infoCont {
	overflow:hidden;
}
.infoItem .itemRight {
	float:right;
	margin-left:0.267rem;
}
.infoItem input {
	width:98%;
	height:0.9rem;
}
/*银行信息 在充值提现 和 实名绑卡页面*/
