.ft dl dd:last-child{border-bottom: 0;}
.ft dl dd .icon_bg{background:url(../images/my/icon_list.png) no-repeat;background-size:0.63rem 3.4rem;display: inline-block;position: relative;top: 0.1rem;left: 0;width: 0.933rem;}
