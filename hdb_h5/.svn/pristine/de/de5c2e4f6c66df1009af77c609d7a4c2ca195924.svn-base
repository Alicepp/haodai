

.list .row {
	height: 1.28rem;
	line-height: 1.28rem;
	font-size: 0.3733rem;
	padding-left: 0.5333rem;
	background: #fff;
	-webkit-box-pack: center;
	-ms-flex-pack: center;
	justify-content: center;
	-webkit-box-align: center;
	-ms-flex-align: center;
	-ms-grid-row-align: center;
	align-items: center;
	padding-right: 0.5333rem;
	border-bottom: 0.0133rem #e8ecf0 solid;
}
.list .row:first-child{
	border-top: 0.0133rem #e8ecf0 solid;
}
.list .row .rightCol {
	width: 0.48rem;
	height: 0.48rem;
	-webkit-box-flex: 0;
	-ms-flex: 0 0 0.48rem;
	flex: 0 0 0.48rem;
	background: url(../images/common/tr.png) center no-repeat;
	background-size: 0.19rem;
}